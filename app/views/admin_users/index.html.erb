<%= link_to("<<Back to Menu", admin_path, :class => 'back-link')%>
<div class="users index">
  <h2>Users</h2>
  <%= image_tag('plus_sign.png', :size => '11x11', :alt => '+')%>
  <%= link_to("Add New User", new_admin_user_path, :class => 'action new') %>

  <table class="listing" summary="Admin user list">
    <tr class="header">
      <th>#</th>
      <th>Name</th>
      <th>Email Id</th>
      <th>Username</th>
      <th>Actions</th>
    </tr>
    <% @admin_users.each do |admin_user| %>
    <tr>
      <td><%= admin_user.name %></td>
      <td><%= admin_user.email %></td>
      <td class="center"><%= admin_user.username %></td>
      <td class="actions">
        <%= link_to("Edit", edit_admin_user_path(admin_user), :class => 'action edit') %>
        <%= link_to("Delete", delete_admin_user_path(admin_user), :class => 'action delete') %>
      </td>
    </tr>
    <% end %>
  </table>
</div>
